{
  "name": "revel8-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "5.9.6",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "2.5.1",
    "@types/proxy-addr": "^2.0.3",
    "@types/source-map-support": "^0.5.10",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.2",
    "typescript": "~4.6",
    "youch": "^3.3.3",
    "youch-terminal": "^2.2.3"
  },
  "dependencies": {
    "@adonisjs/ally": "4.1.5",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/lucid": "18.4.0",
    "@adonisjs/redis": "7.3.4",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/view": "^6.2.0",
    "@multiformats/base-x": "^4.0.1",
    "axios": "^1.7.3",
    "login-with-twitter": "^1.2.4",
    "luxon": "^3.4.4",
    "oauth-signature": "^1.5.0",
    "pg": "^8.12.0",
    "pinata": "^1.10.1",
    "pinata-web3": "^0.5.4",
    "pkce-challenge": "^4.1.0",
    "proxy-addr": "^2.0.7",
    "puppeteer": "^24.8.2",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21",
    "viem": "^2.26.3"
  }
}
